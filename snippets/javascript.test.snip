snippet	describe
abbr	describe('', () => ...)
options	head
	describe('${1:#:feature}', () => {
		${0:#:body}
	});

snippet	describe_it
abbr	describe('', () => ...)
options	head
	describe('${1:#:feature}', () => {
		it('${2:#:spec}', () => {
			${0:#:body}
		});
	});

snippet	it
abbr	it('', () => ...)
options	head
	it('${1:#:spec}', () => {
		${0:#:body}
	});

snippet	it_done
abbr	it('', () => ...)
options	head
	it('${1:#:spec}', done => {
		${0:#:body}
	});

