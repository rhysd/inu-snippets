snippet		link
abbr		[]()
	[${1:#:title}](${2:#:URL})

snippet		link_ref
abbr		[]()
	[${1:#:title}][${2:#:ref}]
	
	[$2]: ${3:#:URL}

snippet		link_paste
abbr		[](...)
	[${1}](`getreg('+')=='' ? '<\`2\`>' : getreg('+')`)${0}

snippet		github_link
abbr		[...](https://github.com/...)
	[${1:#:name}](https://github.com/${2:rhysd}/$1)${0}
